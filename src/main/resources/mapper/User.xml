<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.centling.mapper.blog.UserMapper">
        <select id="selectByNameAndId" resultType="java.lang.Integer">
            select count(1) from user where name=#{name} and id!=#{id};
        </select>
    <insert id="insert" parameterType="com.centling.domain.User">
        INSERT into user (userName,passWord,email,createdTime) values(#{userName},#{passWord},#{email},#{createdTime})
    </insert>
</mapper>